{
  "name": "meauxcloud",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npx esbuild src/main.jsx --bundle --outfile=dist/assets/main.js --servedir=dist --watch",
    "build": "npx esbuild src/main.jsx --bundle --outfile=dist/assets/main.js --minify --sourcemap && npm run copy:html",
    "copy:html": "mkdir -p dist && cp dashboard.html index.html login.html dist/ 2>/dev/null || true",
    "preview": "npx serve dist",
    "sync:r2": "node scripts/sync_assets_to_r2.js",
    "deploy:worker": "npx wrangler deploy",
    "deploy:all": "npm run build && npm run sync:r2 && npm run deploy:worker",
    "deploy": "npm run deploy:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "esbuild": "^0.27.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "vite": "^6.4.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.962.0",
    "hono": "^4.11.3",
    "lucide-react": "^0.562.0",
    "mime-types": "^3.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "resend": "^6.6.0",
    "svix": "^1.84.1"
  }
}